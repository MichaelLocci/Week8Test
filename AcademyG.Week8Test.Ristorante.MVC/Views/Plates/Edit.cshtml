@model PlateViewModel

<h1> Edit Plate - @Model.NamePlate</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-controller="Plates" asp-action="Edit">

            <partial name="_Form" model="Model" />

            <div class="form-group">
                <input type="submit" value="Edit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-controller="Plates" asp-action="Index" asp-route-id="@Model.MenuId">Back To List</a>
</div>